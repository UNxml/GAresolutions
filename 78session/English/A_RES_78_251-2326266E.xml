      <p>
        <docNumber>78/251.</docNumber>
        <docTitle refersTo="#resolutionTitle">
          <span class="bold">	Proposed programme budget for 2024: section 26, Palestine refugees</span>
        </docTitle>
      </p>
    </preface>
    <preamble>
      <formula name="enactingFormula" eId="formula_1">
