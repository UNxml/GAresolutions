    <preface>
      <p>
        <docNumber>77/3.</docNumber>
        <docTitle refersTo="#resolutionTitle">
          <span class="bold">	Prévisions révisées concernant les activités menées par les entités des Nations Unies pour atténuer l’insécurité alimentaire mondiale et ses conséquences humanitaires</span>
        </docTitle>
      </p>
    </preface>
    <preamble>
      <formula name="enactingFormula" eId="formula_1">
        <p xml:space="preserve">
          <span class="italic">L’Assemblée générale</span>,</p>
      </formula>
      <container name="Ayant examiné" eId="container_2">
        <p xml:space="preserve">	<span class="italic">Ayant examiné</span> le rapport du Secrétaire général<authorialNote marker="1" placement="bottom" eId="authorialNote_1"><p xml:space="preserve">		<a href="https://undocs.org/fr/A/77/325">A/77/325</a>.</p></authorialNote> et le rapport correspondant du Comité consultatif pour les questions administratives et budgétaires<authorialNote marker="2" placement="bottom" eId="authorialNote_2"><p xml:space="preserve">		<a href="https://undocs.org/fr/A/77/486">A/77/486</a>.</p></authorialNote>, </p>
      </container>
